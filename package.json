{
  "name": "MyStuff",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "typing": "flow check",
    "lint": "eslint index.ios.js src",
    "jscs": "jscs index.ios.js src",
    "check": "npm run jscs && npm run lint"
  },
  "dependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jscs": "^2.0.5",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "flow-bin": "0.17.0",
    "gb-native-router": "^0.4.3",
    "immutable": "^3.7.5",
    "jscs": "^2.5.0",
    "moment": "^2.10.6",
    "react-native": "^0.13.2",
    "react-native-button": "^1.2.1",
    "react-native-image-picker": "^0.9.0",
    "react-native-material-kit": "^0.2.2",
    "react-native-simple-store": "^0.1.0",
    "react-redux": "^3.1.0",
    "redux": "^3.0.4",
    "redux-form": "^2.4.5",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "esprima": "./node_modules/babel-jscs",
    "fileExtensions": [
      ".js",
      ".jsx"
    ],
    "disallowSpaceAfterObjectKeys": null
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "eslint-plugin-react"
    ],
    "rules": {
      "strict": 0,
      "quotes": 0,
      "camelcase": 0,
      "react/jsx-uses-vars": 2,
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ],
      "no-console": 0,
      "react/jsx-uses-react": 2,
      "react/react-in-jsx-scope": 2,
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    },
    "ecmaFeatures": {
      "blockBindings": true,
      "forOf": true,
      "jsx": true
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    }
  }
}
